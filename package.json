{
  "name": "filter-you-tube",
  "displayName": "Filter YouTube Video",
  "version": "0.0.1",
  "description": "Filter YouTube Video by Keywords",
  "author": "wp",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@plasmohq/storage": "^1.15.0",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/supabase-js": "2.39.3",
    "antd": "^5.25.1",
    "antd-style": "^3.7.1",
    "plasmo": "0.90.5",
    "rc-tween-one": "^3.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "oauth2": {
      "client_id": "769449585468-2jk5v0por1eq5sjuii978g8q31u12fg9.apps.googleusercontent.com",
      "scopes": [
        "openid",
        "email"
      ]
    },
    "web_accessible_resources": [
      {
        "resources": [
          "login.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ],
    "host_permissions": [ "https://www.youtube.com/*" ],
    "permissions": [
      "scripting",
      "tabs",
      "storage",
      "activeTab",
      "identity",
      "identity.email"
    ]
  }
}
